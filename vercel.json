{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "pnpm run docs:build",
  "outputDirectory": "docs/.vitepress/dist",
  "devCommand": "pnpm run docs:dev",
  "installCommand": "pnpm install",
  "framework": null,
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}

